<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mi.learn.com"
    android:versionCode="49"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true" >
        <activity
            android:name="mi.email.activity.Welcome"
            android:label="@string/welcome" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mi.email.activity.MailDetails"
            android:label="@string/mail_details" />
        <activity
            android:name="mi.email.activity.ReceiveList"
            android:label="@string/receivelist" />
        <activity
            android:name="mi.email.activity.SendMail"
            android:label="@string/send_email" />
        <!-- <activity android:name="mi.email.core.ReadDatabase"></activity> -->
        <!-- <activity android:name="mi.email.core.WriteDatabase"></activity> -->
        <activity
            android:name="mi.email.activity.ReceiveAndSend"
            android:label="@string/mail_details" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="mi.learn.com.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>